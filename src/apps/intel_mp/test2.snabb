#!../../snabb snsh
local intel = require("apps.intel_mp.intel_mp")
local pci0 = os.getenv("SNABB_PCI_INTEL1G0")
local pci1 = os.getenv("SNABB_PCI_INTEL1G1")
local nic = intel.Intel1g:new({pciaddr = pci0})

nic:unlock_sw_sem()
nic:lock_sw_sem()
if pcall(nic.lock_sw_sem, nic) then
  main.exit(-1)
end
nic:unlock_sw_sem()
nic:lock_sw_sem()
main.exit(0)
